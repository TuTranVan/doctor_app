<tr>
  <td>
    <%= link_to edit_admin_major_path(major), class: "btn btn-primary" do %>
      <em class="fa fa-pencil"></em>
    <% end %>
    <%= link_to admin_major_path(major), method: :delete, class: "btn btn-danger",
      data: {confirm: "Bạn chắc chắn muốn xóa?"} do %>
      <em class="fa fa-trash"></em>
    <% end %>
  </td>
  <td><%= major.name %></td>
</tr>